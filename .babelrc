{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        "transform-function-bind",
        [ "module-resolver", {
          "root": ".",
          "alias": {
            "~": "./src"
          }
        } ]
      ]
    }
  }
}
